# Home page
GET        /language/:lang        org.iatoki.judgels.commons.controllers.Application.changeLanguage(lang: String)

# Map static resources from the /public folder to the /assets URL path
GET        /assets/*file          controllers.Assets.at(path="/public", file)
GET        /vassets/*file         controllers.Assets.versioned(path="/public", file: Asset)

GET        /auth                  org.iatoki.judgels.commons.controllers.JophielClientController.auth(returnUri: String)
GET        /login                  org.iatoki.judgels.commons.controllers.JophielClientController.login()
GET        /logout/:returnUri                  org.iatoki.judgels.commons.controllers.JophielClientController.logout(returnUri: String)